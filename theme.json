{
  "version": "2",
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "settings": {
    "appearanceTools": false,
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "color": {
      "background": false,
      "custom": false,
      "customDuotone": false,
      "customGradient": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "gradients": [
        {
          "gradient": "linear-gradient(45deg,rgb(204,204,204) 0%,rgb(187,187,187) 100%)",
          "slug": "light-to-medium-gray",
          "name": "Light to Medium Gray"
        },
        {
          "gradient": "linear-gradient(45deg,rgb(204,204,204) 0%,rgb(187,187,187) 51%,rgb(170,170,170) 100%)",
          "slug": "light-to-dark-gray",
          "name": "Light to Dark Gray"
        },
        {
          "gradient": "linear-gradient(45deg,rgb(170,170,170) 0%,rgb(187,187,187) 51%,rgb(187,187,187) 100%)",
          "slug": "dark-to-light-gray",
          "name": "Dark to Light Gray"
        }
      ],
      "link": true,
      "palette": [
        {
          "color": "#ffffff",
          "slug": "white",
          "name": "White"
        },
        {
          "color": "#21759b",
          "slug": "medium-blue",
          "name": "Medium Blue"
        },
        {
          "color": "#000000",
          "slug": "black",
          "name": "Black"
        },
        {
          "color": "#cccccc",
          "slug": "light-gray",
          "name": "Light Gray"
        },
        {
          "color": "#777777",
          "slug": "dark-gray",
          "name": "Dark Gray"
        },
        {
          "color": "#f1f1f1",
          "slug": "lighter-gray",
          "name": "Lighter Gray"
        },
        {
          "color": "#3b3b3b",
          "slug": "blackish",
          "name": "Blackish"
        },
        {
          "color": "#666666",
          "slug": "medium-gray",
          "name": "Medium Gray"
        }
      ],
      "text": false
    },
    "custom": {
      "spacing": {
        "small": "max(1.25rem, 5vw)",
        "medium": "clamp(2rem, 8vw, calc(4 * var(--wp--style--block-gap)))",
        "large": "clamp(4rem, 10vw, 8rem)",
        "outer": "var(--wp--custom--spacing--small, 1.25rem)"
      },
      "typography": {
        "fontSize": {
          "gigantic": "clamp(3rem, 6vw, 4rem)",
          "colossal": "clamp(4rem, 8vw, 6.25rem)"
        },
        "lineHeight": {
          "tiny": 1.15,
          "small": 1.2,
          "medium": 1.4,
          "normal": 1.6
        }
      }
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1240px"
    },
    "spacing": {
      "blockGap": null,
      "margin": false,
      "padding": true,
      "units": [
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, Oxygen-Sans, Ubuntu, Cantarell, \"Helvetica Neue\", sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFamily": "monaco, consolas, \"Andale Mono\", \"DejaVu Sans Mono\", monospace",
          "name": "Code",
          "slug": "code"
        },
        {
          "fontFamily": "\"Courier 10 Pitch\", courier, monospace",
          "name": "Pre",
          "slug": "pre"
        }
      ],
      "fontSizes": [
        {
          "name": "Extra Small",
          "size": "1rem",
          "slug": "extra-small"
        },
        {
          "name": "Small",
          "size": "1.125rem",
          "slug": "small"
        },
        {
          "name": "Normal",
          "size": "1.25rem",
          "slug": "normal"
        },        
        {
          "name": "Large",
          "size": "1.5rem",
          "slug": "large"
        },
        {
          "name": "Extra Large",
          "size": "2.5rem",
          "slug": "extra-large"
        }
      ],
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "lineHeight": true,
      "textDecoration": false,
      "textTransform": false
    },
	"custom": {
		"line-height": {
			"heading": 1.5,
			"page-title": 1.5
		},
		"spacing": {
			"unit": "20px",
			"horizontal": "25px",
			"vertical": "30px"
		},
		"font-weight":{
			"light": "300",
			"normal": "normal",
			"bold": "800"
		}
	}    
  },
  "styles": {
	"color": {
		"background": "var(--wp--preset--color--white)",
		"text": "var(--wp--preset--color--blackish)"
	},
	"typography": {
		"fontSize": "var(--wp--preset--font-size--normal)",
		"lineHeight": 1.5
	},
    "elements": {
		"link": {
			"color": {
				"text": "var(--wp--preset--color--dark-gray)"
			}
		},
		"h1": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--extra-large)",
				"lineHeight": "var(--wp--custom--line-height--page-title)",
				"fontWeight": "var(--wp--custom--font-weight--bold)"
			}
		},
		"h2": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--large)",
				"lineHeight": "var(--wp--custom--line-height--heading)",
				"fontWeight": "var(--wp--custom--font-weight--bold)"
			}
		},
		"h3": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--normal)",
				"lineHeight": "var(--wp--custom--line-height--heading)",
				"fontWeight": "var(--wp--custom--font-weight--normal)"
			}
		},
		"h4": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--normal)",
				"lineHeight": "var(--wp--custom--line-height--heading)"
			}
		},
		"h5": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--small)",
				"lineHeight": "var(--wp--custom--line-height--heading)"
			}
		},
		"h6": {
			"typography": {
				"fontSize": "var(--wp--preset--font-size--extra-small)",
				"lineHeight": "var(--wp--custom--line-height--heading)"
			}
		}		
    }				      
  },
  "customTemplates": [
    {
      "name": "page-home",
      "title": "Page Without Title",
      "postTypes": [
        "page"
      ]
    }
  ],
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ]
}